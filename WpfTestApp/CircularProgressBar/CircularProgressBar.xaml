             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfTestApp.CircularProgressBar"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <Viewbox>
        <Grid Width="20" Height="20">
            <Path Name="myCycleProcessBar1" Data="M5,3 A1,1 0 0 1 4,3" Stroke="Red" StrokeThickness="0.1" Height="8" Width="8" VerticalAlignment="Center" HorizontalAlignment="Center"/>
            <!--<Label Name="lbValue" Content="50%" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="9" />-->
        </Grid>
    </Viewbox>

</UserControl>
